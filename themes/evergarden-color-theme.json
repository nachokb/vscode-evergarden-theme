{
  "name": "Evergarden",
  "type": "dark",
  "colors": {
    "editor.background": "#252B2E",
    "editor.foreground": "#D6CBB4",
    "editorCursor.foreground": "#D6CBB4",
    "editor.selectionBackground": "#3A4045",
    "editor.selectionHighlightBackground": "#3A4045",
    "editor.lineHighlightBackground": "#2A3033",
    "editorLineNumber.foreground": "#859289",
    "editorLineNumber.activeForeground": "#D6CBB4",
    "editorIndentGuide.background": "#3A4045",
    "editorIndentGuide.activeBackground": "#859289",
    "editorWhitespace.foreground": "#3A4045",
    "editorRuler.foreground": "#3A4045",
    "editorCodeLens.foreground": "#859289",
    "scrollbarSlider.background": "#3A404580",
    "scrollbarSlider.hoverBackground": "#3A404590",
    "scrollbarSlider.activeBackground": "#859289",
    "minimap.background": "#252B2E",
    "sideBar.background": "#1F2428",
    "sideBar.foreground": "#D6CBB4",
    "sideBarSectionHeader.background": "#252B2E",
    "sideBarSectionHeader.foreground": "#D6CBB4",
    "activityBar.background": "#1F2428",
    "activityBar.foreground": "#D6CBB4",
    "activityBarBadge.background": "#E67E80",
    "activityBarBadge.foreground": "#252B2E",
    "statusBar.background": "#1F2428",
    "statusBar.foreground": "#D6CBB4",
    "statusBar.noFolderBackground": "#1F2428",
    "statusBar.debuggingBackground": "#E67E80",
    "statusBar.debuggingForeground": "#252B2E",
    "titleBar.activeBackground": "#1F2428",
    "titleBar.activeForeground": "#D6CBB4",
    "titleBar.inactiveBackground": "#1F2428",
    "titleBar.inactiveForeground": "#859289",
    "tab.activeBackground": "#252B2E",
    "tab.activeForeground": "#D6CBB4",
    "tab.inactiveBackground": "#1F2428",
    "tab.inactiveForeground": "#859289",
    "tab.border": "#3A4045",
    "panel.background": "#1F2428",
    "panel.border": "#3A4045",
    "panelTitle.activeForeground": "#D6CBB4",
    "panelTitle.inactiveForeground": "#859289",
    "terminal.background": "#252B2E",
    "terminal.foreground": "#D6CBB4",
    "terminal.ansiBlack": "#252B2E",
    "terminal.ansiRed": "#E67E80",
    "terminal.ansiGreen": "#B2C98F",
    "terminal.ansiYellow": "#DBBC7F",
    "terminal.ansiBlue": "#95aee1",
    "terminal.ansiMagenta": "#D699B6",
    "terminal.ansiCyan": "#7a8478",
    "terminal.ansiWhite": "#D6CBB4",
    "terminal.ansiBrightBlack": "#859289",
    "terminal.ansiBrightRed": "#E67E80",
    "terminal.ansiBrightGreen": "#B2C98F",
    "terminal.ansiBrightYellow": "#DBBC7F",
    "terminal.ansiBrightBlue": "#95aee1",
    "terminal.ansiBrightMagenta": "#D699B6",
    "terminal.ansiBrightCyan": "#7a8478",
    "terminal.ansiBrightWhite": "#D6CBB4"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#859289",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Preprocessor Comment",
      "scope": [
        "comment.block.preprocessor",
        "comment.line.preprocessor"
      ],
      "settings": {
        "foreground": "#E67E80"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#E67E80"
      }
    },
    {
      "name": "Keyword Type",
      "scope": [
        "keyword.type",
        "storage.type.primitive",
        "support.type",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#DBBC7F"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#B2C98F"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "variable.other",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#D6CBB4"
      }
    },
    {
      "name": "Built-in",
      "scope": [
        "support.function.builtin",
        "support.constant.builtin",
        "variable.language"
      ],
      "settings": {
        "foreground": "#D699B6"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "constant.numeric",
        "constant.language",
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#D699B6"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float"
      ],
      "settings": {
        "foreground": "#D699B6"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#B2C98F"
      }
    },
    {
      "name": "String Symbol",
      "scope": [
        "string.quoted.symbol",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#E69875"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#7a8478"
      }
    },
    {
      "name": "Attribute",
      "scope": [
        "entity.other.attribute-name",
        "meta.attribute"
      ],
      "settings": {
        "foreground": "#D699B6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#7a8478"
      }
    },
    {
      "name": "Entity",
      "scope": [
        "entity.name",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#DBBC7F"
      }
    },
    {
      "name": "Exception",
      "scope": [
        "entity.name.exception",
        "support.type.exception"
      ],
      "settings": {
        "foreground": "#E67E80"
      }
    },
    {
      "name": "Label",
      "scope": [
        "entity.name.label"
      ],
      "settings": {
        "foreground": "#E67E80"
      }
    },
    {
      "name": "Diff Deleted",
      "scope": [
        "markup.deleted",
        "markup.deleted.diff"
      ],
      "settings": {
        "foreground": "#252B2E",
        "background": "#E67E80"
      }
    },
    {
      "name": "Diff Inserted",
      "scope": [
        "markup.inserted",
        "markup.inserted.diff"
      ],
      "settings": {
        "foreground": "#252B2E",
        "background": "#B2C98F"
      }
    },
    {
      "name": "Markup Heading",
      "scope": [
        "markup.heading",
        "entity.name.section"
      ],
      "settings": {
        "foreground": "#D699B6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Subheading",
      "scope": [
        "markup.heading.2",
        "markup.heading.3",
        "markup.heading.4",
        "markup.heading.5",
        "markup.heading.6"
      ],
      "settings": {
        "foreground": "#B2C98F",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Emphasis",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#6E8585"
      }
    },
    {
      "name": "Markup Strong",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#D6CBB4"
      }
    },
    {
      "name": "Error",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#252B2E",
        "background": "#E67E80",
        "fontStyle": "bold"
      }
    }
  ]
}